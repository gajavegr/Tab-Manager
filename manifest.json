{
    "manifest_version": 3,
    "name": "Tab Manager",
    "version": "1.0.1",
    "description": "Export and import open tabs",
    "permissions": [
      "tabs",
      "downloads",
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon-16.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "images/icon-16.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  }
  